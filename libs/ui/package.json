      "optional": true
    },
    "styled-components": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.25",
    "@xstyled/styled-components": "^3.6.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "styled-components": "^5.3.5",
